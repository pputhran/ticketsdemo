<!DOCTYPE html>
<html ng-app="trackerApp">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Issue Tracker</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular.min.js"></script>
    <script src="/javascripts/ngApp.js"></script>
    <script src="http://d3js.org/d3.v3.min.js"></script>
    <script src="/javascripts/d3pie.js"></script>
</head>
<body ng-controller="TableController">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button>
                <a href="/" class="navbar-brand">Issue Tracker</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="/">Home</a></li>                    
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">        
        @html.block('content')
        <hr>
        <footer>
            <p>&copy; @model.year</p>
        </footer>
    </div>
    
    <script src="/javascripts/modernizr-2.6.2.js"></script>
    <script src="/javascripts/jquery-1.10.2.js"></script>
    <script src="/javascripts/bootstrap.js"></script>
    <script src="/javascripts/respond.js"></script>

    @html.block('scripts')
</body>
</html>
